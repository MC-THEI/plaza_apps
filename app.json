{
  "expo": {
    "name": "Plaza-Apps",
    "slug": "Plaza-Apps",
    "version": "1.0.0",
    "backgroundColor": "#3D3D3D",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3D3D3D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theider90.PlazaApps"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3D3D3D"
      },
      "package": "com.theider90.PlazaApps"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-font",
      "expo-asset",
    ],
    "extra": {
      "eas": {
        "projectId": "ffa0d5a9-7657-4118-899d-2c6e34943740"
      }
    }
  }
}
